{
  "name": "@kvkit/query",
  "version": "0.1.0",
  "description": "Encode/decode URLSearchParams via codecs",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
